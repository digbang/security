{
	"name": "digbang/security",
	"license": "MIT",
	"description": "Security package for Laravel 5 projects in Digbang.",
	"authors": [
		{
			"name": "Guido Contreras Woda",
			"email": "gcontreras@digbang.com"
		}
	],
	"require": {
		"php": ">=7.1",
		"laravel-doctrine/orm": "1.3.*|1.4.*",
		"laravel-doctrine/fluent": "^1.1",
		"illuminate/support":   "5.4.*|5.5.*|5.6.*",
		"illuminate/contracts": "5.4.*|5.5.*|5.6.*",
		"illuminate/events":    "5.4.*|5.5.*|5.6.*",
		"illuminate/cookie":    "5.4.*|5.5.*|5.6.*",
		"illuminate/routing":   "5.4.*|5.5.*|5.6.*",
		"cartalyst/sentinel":   "~2.0",
		"doctrine/dbal":        "~2.5"
	},
	"require-dev": {
		"phpspec/phpspec":     "^3.0",
		"henrikbjorn/phpspec-code-coverage": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"Digbang\\Security\\": "src/"
		}
	},
	"repositories": [],
	"minimum-stability": "dev",
	"prefer-stable": true
}
